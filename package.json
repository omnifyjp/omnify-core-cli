{
  "name": "@famgia/omnify-cli",
  "version": "0.0.169",
  "description": "CLI interface for omnify-schema",
  "type": "module",
  "bin": {
    "omnify": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@famgia/omnify-atlas": "workspace:*",
    "@famgia/omnify-core": "workspace:*",
    "@famgia/omnify-laravel": "workspace:*",
    "@famgia/omnify-typescript": "workspace:*",
    "@famgia/omnify-types": "workspace:*",
    "@inquirer/prompts": "^8.1.0",
    "commander": "^14.0.2",
    "jiti": "^2.6.1",
    "picocolors": "^1.1.1"
  }
}
